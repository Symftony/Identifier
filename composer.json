{
    "name": "symftony/identifier",
    "authors": [
        {
            "name": "Anthony",
            "email": "symftony@gmail.com"
        }
    ],
    "homepage": "https://github.com/symftony/Identifier",
    "description": "A simple library to generate simple or complex identifier.",
    "keywords": ["identifier","token","tracker"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "ramsey/uuid": "^3.7"
    },
    "suggest": {
        "ramsey/uuid": "Provide RFC 4122 version 1, 3, 4, and 5 universally unique identifiers (UUID)."
    },
    "autoload": {
        "psr-4": {
            "Symftony\\Identifier\\": "src/Identifier"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symftony\\Identifier\\Tests\\": "tests/Identifier"
        }
    }
}
